{
  "version": 2,
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "devCommand": "npm run web",
  "framework": null,
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "EXPO_PUBLIC_FIREBASE_API_KEY": "@firebase_api_key",
    "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "@firebase_auth_domain",
    "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "@firebase_project_id",
    "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "@firebase_storage_bucket",
    "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "@firebase_messaging_sender_id",
    "EXPO_PUBLIC_FIREBASE_APP_ID": "@firebase_app_id",
    "EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID": "@firebase_measurement_id",
    "EXPO_PUBLIC_GOOGLE_CLIENT_ID_WEB": "@google_client_id_web",
    "EXPO_PUBLIC_GOOGLE_CLIENT_ID_IOS": "@google_client_id_ios",
    "EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID": "@google_client_id_android",
    "EXPO_PUBLIC_GITHUB_CLIENT_ID": "@github_client_id",
    "EXPO_PUBLIC_GITHUB_CLIENT_SECRET": "@github_client_secret",
    "EXPO_PUBLIC_GITHUB_REDIRECT_URI": "@github_redirect_uri",
    "EXPO_PUBLIC_N8N_WEBHOOK_URL": "@n8n_webhook_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

